generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("PROD_PRISMA_URL") // uses connection pooling
  directUrl = env("PROD_URL_NON_POOLING") // uses a direct connection
  shadowDatabaseUrl = env("PROD_URL_NON_POOLING") // used for migrations
}

model order {
  order_id    Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  quality     String?
  quantity    String?
  rate        String?
  weaverName  String?
  partyName   String?
  payment     String?
  weaverPrice String?
}
