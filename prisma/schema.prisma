generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = process.env.DATABASE_URL
}

model order {
  order_id    Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  quality     String?
  quantity    String?
  rate        String?
  weaverName  String?
  partyName   String?
  payment     String?
  weaverPrice String?
}
